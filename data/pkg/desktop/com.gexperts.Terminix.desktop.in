[Desktop Entry]
Version=1.0
Name=Terminix
Comment=A tiling terminal for Gnome
Keywords=shell;prompt;command;commandline;cmd;
Exec=terminix
Terminal=false
Type=Application
StartupNotify=true
Categories=System;TerminalEmulator;X-GNOME-Utilities;
Icon=com.gexperts.Terminix
DBusActivatable=true
Actions=new-window;new-session;
StartupWMClass=terminix

[Desktop Action new-window]
Name=New Window
Exec=terminix --action=app-new-window

[Desktop Action new-session]
Name=New Session
Exec=terminix --action=app-new-session
